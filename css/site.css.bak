.navbar-inner-color .nav > li > a {
  float: none;
  padding: 10px 10px 11px;
  line-height: 19px;
  color: #333333;
  text-decoration: none;
  text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
}

.navbar-inner-color .brand {
  float: left;
  display: block;
  padding: 8px 20px 12px;
  margin-left: -20px;
  font-size: 20px;
  font-weight: 200;
  line-height: 1;
  color: #333333;
}

.thumbnails > li {
  background-color: #FFBBFF;
border-color: #DA70D6;
}